{
  "name": "phantom-fe-codebandits",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "engines": {
    "npm": "8.x",
    "node": "16.x"
  },
  "scripts": {
    "start": "webpack serve",
    "build": "cross-env NODE_ENV=production  webpack",
    "prepare": "husky install",
    "format": "npx pretty-quick",
    "lint": "npx eslint src/*.j{s,sx} --fix",
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "testPro": "cd dist && http-server"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "\\.(js|jsx)$": "babel-jest",
      "\\.(jpg|jpeg|png|svg)$": "<rootDir>/config/jest/fileMock.js"
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/index.js",
      "<rootDir>/src/server.js"
    ],
    "collectCoverage": true,
    "testEnvironment": "jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/phantom-fe-codebandits.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/phantom-fe-codebandits/issues"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@reduxjs/toolkit": "^1.7.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "component-connector": "^1.0.5",
    "express": "^4.17.2",
    "file-loader": "^6.2.0",
    "font-awesome": "^4.7.0",
    "react": "^17.0.2",
    "react-content-loader": "^6.1.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.27.1",
    "react-loading-skeleton": "^3.0.3",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-test-renderer": "^17.0.2",
    "react-toastify": "^8.2.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/plugin-transform-async-to-generator": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@testing-library/react-hooks": "^7.0.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jsdom": "^16.7.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^12.3.3",
    "postcss": "^8.4.6",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.1",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "react-validation": "^3.0.7",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.23",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{js,css,md}": "npx pretty-quick"
  }
}